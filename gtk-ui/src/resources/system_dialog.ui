<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="SystemDialog" parent="GtkDialog">
    <property name="title" translatable="yes">Add system</property>
    <child internal-child="content_area">
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <property name="spacing">12</property>
        <property name="margin-top">12</property>
        <property name="margin-bottom">12</property>
        <property name="margin-start">12</property>
        <property name="margin-end">12</property>
        <child>
          <object class="GtkLabel">
            <property name="label" translatable="yes">System name:</property>
            <property name="halign">start</property>
          </object>
        </child>
        <child>
	    <object class="GtkEntry" id="system_name_entry">
            <property name="placeholder-text" translatable="yes">Enter system name</property>
          </object>
        </child>
      </object>
    </child>
    <child type="action">
	    <object class="GtkButton" id="cancel_button">
        <property name="label" translatable="yes">Cancel</property>
      </object>
    </child>
    <child type="action">
	    <object class="GtkButton" id="add_button">
        <property name="label" translatable="yes">Add</property>
      </object>
    </child>
  </template>
</interface>

