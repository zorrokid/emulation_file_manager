<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="MyAppReleaseFormWindow" parent="GtkWindow">
    <property name="title">Add Release</property>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <property name="spacing">6</property>
        <property name="margin">12</property>
        <child>
          <object class="GtkEntry" id="version_entry">
            <property name="placeholder-text">Name</property>
          </object>
        </child>
        <child>
          <object class="GtkEntry" id="date_entry">
            <property name="placeholder-text">Date</property>
          </object>
        </child>
        <child>
          <object class="GtkButton" id="save_button">
            <property name="label">Save</property>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>

